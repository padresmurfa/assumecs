<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageVersion>1.1.3</PackageVersion>
    <Authors>David Brandt</Authors>
    <NeutralLanguage>en-EN</NeutralLanguage>
    <PackageLicenseUrl>http://unlicense.org/</PackageLicenseUrl>
    <Owners>David Brandt</Owners>
    <PackageProjectUrl>https://github.com/padresmurfa/assumptions</PackageProjectUrl>
    <PackageReleaseNotes>This version adds the ability to make assumptions about the occurrance/absence of an exception in an Action</PackageReleaseNotes>
    <Summary>The Assumptions library provides a clean method to make implicit assumptions explicit in C#.</Summary>
    <PackageTags>assumptions assume assert implicit explicit code quality testing</PackageTags>
    <Title>Assumptions</Title>
    <Description>The Assumptions library provides a clean method to make implicit assumptions explicit in C#.</Description>
    <PackageId>Assumptions</PackageId>
    <PackOnBuild>true</PackOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NuGet.Build.Packaging" Version="0.2.0" />
  </ItemGroup>
</Project>
